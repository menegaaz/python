numero = int(input("Digite um número: "))
if numero > 10:
        print("Somente a tabuada até 10")
else:
    print(numero,"X",1,"=",numero)
    mult = 2
    result = numero
    while result < numero * 10:
        result = result + numero
        print(numero,"X",mult,"=",result)
        mult = mult + 1
    else:  
        print("Tabuada criada corretamente")
